{
  "manifest_version": 3,
  "name": "Robux Changer",
  "version": "1.0",
  "description": "Change the visual Robux value on Roblox",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.roblox.com/*"],
      "js": ["content.js"]
    }
  ]
}
